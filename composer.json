{
    "name": "dniccum/nova-documentation",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "cebe/markdown": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Dniccum\\NovaDocumentation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dniccum\\NovaDocumentation\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
